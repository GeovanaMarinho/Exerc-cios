/*Escreva um programa em C que recebe um número “n” do usuário e imprime todos os divisores de 5 no intervalo [1, n].*/
#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
int main()
{
    setlocale(LC_ALL, "portuguese");
    int n;
    printf("Insira um valor qualquer: ");
    scanf("%d", &n);
    for (; n >= 1; n--){
        if(5%n==0){
        printf("UM DOS DIVISORES DE CINCO É: %d\n", n);
        }
    }
    return 0;
}
